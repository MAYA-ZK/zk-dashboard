{
  "plugins": [
    "prettier-plugin-sql",
    "prettier-plugin-embed",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "arrowParens": "always",
  "endOfLine": "lf",
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "embeddedSqlTags": ["sql"],
  "language": "postgresql",
  "importOrder": ["^@zk-dashboard/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
